            app:destination="@id/SecondFragment"/>
        <action
            android:id="@+id/action_UsersFragment_to_EditFragment"
            app:destination="@id/EditFragment"/>

    </fragment>

    <fragment
        android:id="@+id/AddPetFragment"
        android:name="elfak.mosis.petfinder.AddPetFragment"
