            android:id="@+id/action_fragmentEditProfile_to_MapFragment"
            app:destination="@id/MapFragment"/>
    </fragment>

    <fragment
        android:id="@+id/EditFragment"
        android:name="elfak.mosis.petfinder.EditFragment"
